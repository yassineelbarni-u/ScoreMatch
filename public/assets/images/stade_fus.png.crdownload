            socialDiv.addEventListener('click', function(event) {
                // Check if the clicked element has a class that matches your share buttons
                if (event.target.classList.contains('tw-post')) {
                    sendShareEvent('Twitter');
                } else if (event.target.classList.contains('fb-post')) {
                    sendShareEvent('Facebook');
                } else if (event.target.classList.contains('in-post')) {
                    sendShareEvent('LinkedIn');
                } else if (event.target.classList.contains('mail-post')) {
                    sendShareEvent('Email');
                }
            });

